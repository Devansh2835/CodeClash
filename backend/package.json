{
 "name": "code-battle-backend",
 "version": "1.0.0",
 "description": "Backend server for 1v1 code battle platform",
 "main": "dist/server.js",
 "scripts": {
  "dev": "nodemon --exec ts-node src/server.ts",
  "build": "tsc",
  "start": "node dist/server.js",
  "seed": "ts-node src/scripts/seed.ts",
  "test-ai": "ts-node scripts/testAiAgent.ts",
  "test": "jest --watchAll=false",
  "test:watch": "jest --watch",
  "lint": "eslint src/**/*.ts",
  "format": "prettier --write \"src/**/*.ts\""
 },
 "keywords": [
  "coding",
  "competition",
  "1v1",
  "judge",
  "battle"
 ],
 "author": "Your Name",
 "license": "MIT",
 "dependencies": {
  "axios": "^1.6.2",
  "bcrypt": "^5.1.1",
  "cookie-parser": "^1.4.6",
  "cors": "^2.8.5",
  "dotenv": "^16.3.1",
  "express": "^4.18.2",
  "express-rate-limit": "^7.1.5",
  "helmet": "^7.1.0",
  "jsonwebtoken": "^9.0.2",
  "mongoose": "^8.0.3",
  "nodemailer": "^7.0.10",
  "redis": "^4.6.12",
  "socket.io": "^4.6.1",
  "winston": "^3.11.0",
  "zod": "^3.22.4"
 },
 "devDependencies": {
  "@types/bcrypt": "^5.0.2",
  "@types/cookie-parser": "^1.4.6",
  "@types/cors": "^2.8.17",
  "@types/express": "^4.17.21",
  "@types/jest": "^29.5.11",
  "@types/jsonwebtoken": "^9.0.5",
  "@types/node": "^20.10.6",
  "@types/nodemailer": "^7.0.4",
  "@typescript-eslint/eslint-plugin": "^6.17.0",
  "@typescript-eslint/parser": "^6.17.0",
  "eslint": "^8.56.0",
  "jest": "^29.7.0",
  "nodemon": "^3.0.2",
  "prettier": "^3.1.1",
  "supertest": "^7.1.3",
  "ts-jest": "^29.1.1",
  "ts-node": "^10.9.2",
  "typescript": "^5.3.3"
 },
 "engines": {
  "node": ">=18.0.0",
  "npm": ">=9.0.0"
 }
}
